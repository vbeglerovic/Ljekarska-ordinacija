<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #BBFFFF;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tableViewAppointments" layoutX="76.0" layoutY="122.0" prefHeight="383.0" prefWidth="637.0" style="-fx-background-color: #00BFFF;">
        <columns>
           <TableColumn fx:id="colAppointmentDate" prefWidth="100.0" text="%date" />
           <TableColumn fx:id="colAppointmentTime" minWidth="0.0" prefWidth="85.0" text="%time" />
          <TableColumn fx:id="colAppointmentDoctor" prefWidth="154.0" text="%doctor" />
            <TableColumn fx:id="colAppointmentPatient" prefWidth="150.0" text="%patient" />
          <TableColumn fx:id="colAppointmentType" prefWidth="147.0" text="%type" />

        </columns>
      </TableView>
      <ToolBar layoutX="240.0" layoutY="532.0" prefHeight="45.0" prefWidth="530.0" style="-fx-background-color: #00BFFF;">
         <items>
            <Button mnemonicParsing="false" onAction="#deleteAppointmentAction" prefHeight="30.0" prefWidth="84.0" text="%delete">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#editAppointmentAction" prefHeight="30.0" prefWidth="70.0" text="%edit">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#addReportAction" prefHeight="30.0" prefWidth="134.0" text="%add.report">
               <font>
                  <Font size="14.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#viewReportAction" prefHeight="30.0" prefWidth="141.0" text="%view.report">
               <font>
                  <Font size="14.0" />
               </font></Button>
          <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeAction" text="%cancel">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </items>
      </ToolBar>
      <ToolBar layoutX="27.0" layoutY="40.0" prefHeight="52.0" prefWidth="755.0" style="-fx-background-color: #00BFFF;">
         <items>
            <TextField fx:id="doctorFld" prefHeight="35.0" prefWidth="165.0" promptText="%doctor">
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <TextField fx:id="patientFld" prefHeight="35.0" prefWidth="165.0" promptText="%patient">
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <TextField fx:id="d1Fld" prefHeight="32.0" prefWidth="152.0" promptText="%from">
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <Label text="-" />
            <TextField fx:id="d2Fld" prefHeight="33.0" prefWidth="152.0" promptText="%to">
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <Button mnemonicParsing="false" onAction="#search" prefHeight="31.0" prefWidth="81.0" text="%search">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </items>
      </ToolBar>
   </children>
</AnchorPane>
