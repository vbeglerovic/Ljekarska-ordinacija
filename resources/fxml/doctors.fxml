                <TableColumn fx:id="colDoctorName" prefWidth="66.0" text="Ime" />
                <TableColumn fx:id="colDoctorLastName" prefWidth="84.0" text="Prezime" />
                <TableColumn fx:id="colDoctorJMBG" prefWidth="75.0" text="JMBG" />
                <TableColumn fx:id="colDoctorDOB" prefWidth="96.0" text="Datum rodjenja" />
                <TableColumn fx:id="colDoctorPOB" prefWidth="108.0" text="Mjesto rodjenja" />
