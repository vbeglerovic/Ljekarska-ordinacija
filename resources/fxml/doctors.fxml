            </items>
        </ToolBar>
        <TextField fx:id="searchFld" layoutX="128.0" layoutY="78.0" prefHeight="25.0" prefWidth="218.0" promptText="ime i prezime" />
      <Label layoutX="78.0" layoutY="78.0" prefHeight="25.0" prefWidth="36.0" text="Search" />
    </children>
</AnchorPane>
