                <TableColumn fx:id="colDoctorDOE" prefWidth="108.0" text="Datum zaposlenja" />
                <TableColumn fx:id="colDoctorEmail" prefWidth="98.0" text="Email" />
                <TableColumn fx:id="colDoctorSpecialty" prefWidth="75.0" text="Specijalnost" />



            </columns>
        </TableView>
        <ToolBar layoutX="301.0" layoutY="340.0" prefHeight="40.0" prefWidth="278.0" style="-fx-background-color: #00BFFF;">
