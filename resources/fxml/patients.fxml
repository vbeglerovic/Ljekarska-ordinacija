<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #BBFFFF;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tableViewPatients" layoutX="11.0" layoutY="158.0" prefHeight="363.0" prefWidth="780.0" style="-fx-background-color: #00BFFF;">
        <columns>
          <TableColumn fx:id="colPatientName" prefWidth="75.0" text="%first.name" />
          <TableColumn fx:id="colPatientLastName" prefWidth="75.0" text="%last.name" />
          <TableColumn fx:id="colPatientJMBG" prefWidth="78.0" text="%jmb" />
            <TableColumn fx:id="colPatientGender" prefWidth="67.0" text="%gender" />
          <TableColumn fx:id="colPatientDOB" prefWidth="84.0" text="%birth.date" />
          <TableColumn fx:id="colPatientPOB" prefWidth="77.0" text="%birth.place" />
            <TableColumn fx:id="colPatientAddress" prefWidth="107.0" text="%address" />
            <TableColumn fx:id="colPatientStatus" prefWidth="78.0" text="%status" />
            <TableColumn fx:id="colPatientEmail" minWidth="0.0" prefWidth="138.0" text="%email" />


        </columns>
      </TableView>
      <ToolBar layoutX="345.0" layoutY="532.0" prefHeight="53.0" prefWidth="432.0" style="-fx-background-color: #00BFFF;">
        <items>
            <Button mnemonicParsing="false" onAction="#printReportAction" prefHeight="31.0" prefWidth="90.0" text="%report" />
          <Button onAction="#editPatientAction" prefHeight="33.0" prefWidth="72.0" text="%edit" />
            <Button onAction="#deletePatientAction" prefHeight="31.0" prefWidth="78.0" text="%delete" />
            <Button onAction="#addPatientAction" prefHeight="33.0" prefWidth="74.0" text="%add" />
            <Button fx:id="closeButton" onAction="#closeAction" prefHeight="30.0" prefWidth="81.0" text="%cancel" />
        </items>
      </ToolBar>
      <TextField fx:id="searchFld" layoutX="127.0" layoutY="101.0" prefHeight="32.0" prefWidth="232.0" promptText="%name.surname">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Label layoutX="48.0" layoutY="101.0" prefHeight="32.0" prefWidth="71.0" text="%search">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="323.0" layoutY="28.0" text="%patients">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
