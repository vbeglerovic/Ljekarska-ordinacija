<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="702.0" style="-fx-background-color: #BBFFFF;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="tableViewPatients" layoutX="10.0" layoutY="102.0" prefHeight="212.0" prefWidth="670.0" style="-fx-background-color: #00BFFF;">
        <columns>
          <TableColumn fx:id="colPatientName" prefWidth="75.0" text="Ime" />
          <TableColumn fx:id="colPatientLastName" prefWidth="75.0" text="Prezime" />
          <TableColumn fx:id="colPatientJMBG" prefWidth="75.0" text="JMBG" />
            <TableColumn fx:id="colPatientGender" prefWidth="75.0" text="Spol" />
          <TableColumn fx:id="colPatientDOB" prefWidth="96.0" text="Datum rodjenja" />
          <TableColumn fx:id="colPatientPOB" prefWidth="108.0" text="Mjesto rodjenja" />
            <TableColumn fx:id="colPatientAddress" prefWidth="75.0" text="Adresa" />
            <TableColumn fx:id="colPatientStatus" prefWidth="75.0" text="Status" />
            <TableColumn fx:id="colPatientEmail" prefWidth="90.0" text="Email" />


        </columns>
      </TableView>
      <ToolBar layoutX="459.0" layoutY="332.0" prefHeight="40.0" prefWidth="218.0" style="-fx-background-color: #00BFFF;">
        <items>
          <Button onAction="#editPatientAction" text="Uredi" />
            <Button onAction="#deletePatientAction" text="Obrisi" />
            <Button onAction="#addPatientAction" text="Dodaj" />
            <Button onAction="#closeAction" text="Cancel" />
        </items>
      </ToolBar>
      <TextField fx:id="searchFld" layoutX="365.0" layoutY="41.0" prefHeight="25.0" prefWidth="218.0" />
      <ToolBar layoutX="605.0" layoutY="33.0" prefHeight="40.0" prefWidth="72.0" style="-fx-background-color: #00BFFF;">
        <items>
          <Button mnemonicParsing="false" text="Pretrazi" />
        </items>
      </ToolBar>
   </children>
</AnchorPane>
